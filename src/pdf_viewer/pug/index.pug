doctype html
html(dir="ltr" mozdisallowselectionprint)
	head
		meta(charset='utf-8')
		meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1')
		meta(name='google' content='notranslate')
		title PDF.js viewer
		link(rel='stylesheet' href="pdf_main.css?" + hash)
		link(rel='resource' type='application/l10n' href='pdf.js/web/locale/locale.properties')
	body.viewerEmbeded(tabindex="1")
		// outerContainer
		#outerContainer
			// sidebarContainer
			#sidebarContainer
				#toolbarSidebar
					#toolbarSidebarLeft
						#sidebarViewButtons.splitToolbarButton.toggled(role='radiogroup')
							button#viewThumbnail.toolbarButton.toggled(title='Show Thumbnails' tabindex='2' data-l10n-id='thumbs' role='radio' aria-checked='true' aria-controls='thumbnailView')
								span(data-l10n-id='thumbs_label') Thumbnails
							button#viewOutline.hidden.toolbarButton(title='Show Document Outline (double-click to expand/collapse all items)' tabindex='3' data-l10n-id='document_outline' role='radio' aria-checked='false' aria-controls='outlineView')
								span(data-l10n-id='document_outline_label') Document Outline
							button#viewAttachments.hidden.toolbarButton(title='Show Attachments' tabindex='4' data-l10n-id='attachments' role='radio' aria-checked='false' aria-controls='attachmentsView')
								span(data-l10n-id='attachments_label') Attachments
							button#viewLayers.hidden.toolbarButton(title='Show Layers (double-click to reset all layers to the default state)' tabindex='5' data-l10n-id='layers' role='radio' aria-checked='false' aria-controls='layersView')
								span(data-l10n-id='layers_label') Layers
					#toolbarSidebarRight
						#outlineOptionsContainer.hidden
							.verticalToolbarSeparator
							button#currentOutlineItem.toolbarButton(disabled='disabled' title='Find Current Outline Item' tabindex='6' data-l10n-id='current_outline_item')
								span(data-l10n-id='current_outline_item_label') Current Outline Item
				#sidebarContent
					#thumbnailView
					#outlineView.hidden
					#attachmentsView.hidden
					#layersView.hidden
				#sidebarResizer
			// End sidebarContainer
			// mainContainer
			#mainContainer
				// findbar
				#findbar.findbar.hidden.doorHanger
					#findbarInputContainer
						input#findInput.toolbarField(title='Find' placeholder='Find in document…' tabindex='91' data-l10n-id='find_input' aria-invalid='false')
						.splitToolbarButton
							button#findPrevious.toolbarButton(title='Find the previous occurrence of the phrase' tabindex='92' data-l10n-id='find_previous')
								span(data-l10n-id='find_previous_label') Previous
							.splitToolbarButtonSeparator
							button#findNext.toolbarButton(title='Find the next occurrence of the phrase' tabindex='93' data-l10n-id='find_next')
								span(data-l10n-id='find_next_label') Next
					#findbarOptionsOneContainer
						input#findHighlightAll.toolbarField(type='checkbox' tabindex='94')
						label.toolbarLabel(for='findHighlightAll' data-l10n-id='find_highlight') Highlight All
						input#findMatchCase.toolbarField(type='checkbox' tabindex='95')
						label.toolbarLabel(for='findMatchCase' data-l10n-id='find_match_case_label') Match Case
					#findbarOptionsTwoContainer
						input#findMatchDiacritics.toolbarField(type='checkbox' tabindex='96')
						label.toolbarLabel(for='findMatchDiacritics' data-l10n-id='find_match_diacritics_label') Match Diacritics
						input#findEntireWord.toolbarField(type='checkbox' tabindex='97')
						label.toolbarLabel(for='findEntireWord' data-l10n-id='find_entire_word_label') Whole Words
					#findbarMessageContainer(aria-live='polite')
						span#findResultsCount.toolbarLabel
						span#findMsg.toolbarLabel
				// End findbar
				#editorFreeTextParamsToolbar.editorParamsToolbar.hidden.doorHangerRight
					.editorParamsToolbarContainer
						.editorParamsSetter
							label.editorParamsLabel(for='editorFreeTextColor' data-l10n-id='editor_free_text_color') Color
							input#editorFreeTextColor.editorParamsColor(type='color' tabindex='100')
						.editorParamsSetter
							label.editorParamsLabel(for='editorFreeTextFontSize' data-l10n-id='editor_free_text_size') Size
							input#editorFreeTextFontSize.editorParamsSlider(type='range' value='10' min='5' max='100' step='1' tabindex='101')
				#editorInkParamsToolbar.editorParamsToolbar.hidden.doorHangerRight
					.editorParamsToolbarContainer
						.editorParamsSetter
							label.editorParamsLabel(for='editorInkColor' data-l10n-id='editor_ink_color') Color
							input#editorInkColor.editorParamsColor(type='color' tabindex='102')
						.editorParamsSetter
							label.editorParamsLabel(for='editorInkThickness' data-l10n-id='editor_ink_thickness') Thickness
							input#editorInkThickness.editorParamsSlider(type='range' value='1' min='1' max='20' step='1' tabindex='103')
						.editorParamsSetter
							label.editorParamsLabel(for='editorInkOpacity' data-l10n-id='editor_ink_opacity') Opacity
							input#editorInkOpacity.editorParamsSlider(type='range' value='100' min='1' max='100' step='1' tabindex='104')
				// secondaryToolbar
				#secondaryToolbar.secondaryToolbar.hidden.doorHangerRight
					#secondaryToolbarButtonContainer
						button#secondaryOpenFile.secondaryToolbarButton.visibleLargeView(title='Open File' tabindex='51' data-l10n-id='open_file')
							span(data-l10n-id='open_file_label') Open
						button#secondaryPrint.secondaryToolbarButton.visibleMediumView(title='Print' tabindex='52' data-l10n-id='print')
							span(data-l10n-id='print_label') Print
						button#secondaryDownload.secondaryToolbarButton.visibleMediumView(title='Save' tabindex='53' data-l10n-id='save')
							span(data-l10n-id='save_label') Save
						.horizontalToolbarSeparator.visibleLargeView
						button#presentationMode.secondaryToolbarButton(title='Switch to Presentation Mode' tabindex='54' data-l10n-id='presentation_mode')
							span(data-l10n-id='presentation_mode_label') Presentation Mode
						a#viewBookmark.secondaryToolbarButton(href='#' title='Current view (copy or open in new window)' tabindex='55' data-l10n-id='bookmark')
							span(data-l10n-id='bookmark_label') Current View
						#viewBookmarkSeparator.horizontalToolbarSeparator
						button#firstPage.secondaryToolbarButton(title='Go to First Page' tabindex='56' data-l10n-id='first_page')
							span(data-l10n-id='first_page_label') Go to First Page
						button#lastPage.secondaryToolbarButton(title='Go to Last Page' tabindex='57' data-l10n-id='last_page')
							span(data-l10n-id='last_page_label') Go to Last Page
						.horizontalToolbarSeparator
						button#pageRotateCw.secondaryToolbarButton(title='Rotate Clockwise' tabindex='58' data-l10n-id='page_rotate_cw')
							span(data-l10n-id='page_rotate_cw_label') Rotate Clockwise
						button#pageRotateCcw.secondaryToolbarButton(title='Rotate Counterclockwise' tabindex='59' data-l10n-id='page_rotate_ccw')
							span(data-l10n-id='page_rotate_ccw_label') Rotate Counterclockwise
						.horizontalToolbarSeparator
						#cursorToolButtons(role='radiogroup')
							button#cursorSelectTool.secondaryToolbarButton.toggled(title='Enable Text Selection Tool' tabindex='60' data-l10n-id='cursor_text_select_tool' role='radio' aria-checked='true')
								span(data-l10n-id='cursor_text_select_tool_label') Text Selection Tool
							button#cursorHandTool.secondaryToolbarButton(title='Enable Hand Tool' tabindex='61' data-l10n-id='cursor_hand_tool' role='radio' aria-checked='false')
								span(data-l10n-id='cursor_hand_tool_label') Hand Tool
						.horizontalToolbarSeparator
						#scrollModeButtons(role='radiogroup')
							button#scrollPage.secondaryToolbarButton(title='Use Page Scrolling' tabindex='62' data-l10n-id='scroll_page' role='radio' aria-checked='false')
								span(data-l10n-id='scroll_page_label') Page Scrolling
							button#scrollVertical.secondaryToolbarButton.toggled(title='Use Vertical Scrolling' tabindex='63' data-l10n-id='scroll_vertical' role='radio' aria-checked='true')
								span(data-l10n-id='scroll_vertical_label') Vertical Scrolling
							button#scrollHorizontal.secondaryToolbarButton(title='Use Horizontal Scrolling' tabindex='64' data-l10n-id='scroll_horizontal' role='radio' aria-checked='false')
								span(data-l10n-id='scroll_horizontal_label') Horizontal Scrolling
							button#scrollWrapped.secondaryToolbarButton(title='Use Wrapped Scrolling' tabindex='65' data-l10n-id='scroll_wrapped' role='radio' aria-checked='false')
								span(data-l10n-id='scroll_wrapped_label') Wrapped Scrolling
						.horizontalToolbarSeparator
						#spreadModeButtons(role='radiogroup')
							button#spreadNone.secondaryToolbarButton.toggled(title='Do not join page spreads' tabindex='66' data-l10n-id='spread_none' role='radio' aria-checked='true')
								span(data-l10n-id='spread_none_label') No Spreads
							button#spreadOdd.secondaryToolbarButton(title='Join page spreads starting with odd-numbered pages' tabindex='67' data-l10n-id='spread_odd' role='radio' aria-checked='false')
								span(data-l10n-id='spread_odd_label') Odd Spreads
							button#spreadEven.secondaryToolbarButton(title='Join page spreads starting with even-numbered pages' tabindex='68' data-l10n-id='spread_even' role='radio' aria-checked='false')
								span(data-l10n-id='spread_even_label') Even Spreads
						.horizontalToolbarSeparator
						button#documentProperties.secondaryToolbarButton(title='Document Properties…' tabindex='69' data-l10n-id='document_properties' aria-controls='documentPropertiesDialog')
							span(data-l10n-id='document_properties_label') Document Properties&mldr;
				// End secondaryToolbar
				.toolbar
					// toolbarTitle
					.toolbarTitle
						#docTitle
						a#downloadTag(data-l10n-id='save_label' aria-expanded='false')
							|Save
					// End toolbarTitle
					// toolbarContainer
					#toolbarContainer
						#toolbarViewer
							#toolbarViewerLeft
								button#sidebarToggle.toolbarButton(title='Toggle Sidebar' tabindex='11' data-l10n-id='toggle_sidebar' aria-expanded='false' aria-controls='sidebarContainer')
									span(data-l10n-id='toggle_sidebar_label') Toggle Sidebar
								.toolbarButtonSpacer
								button#viewFind.toolbarButton(title='Find in Document' tabindex='12' data-l10n-id='findbar' aria-expanded='false' aria-controls='findbar')
									span(data-l10n-id='findbar_label') Find
								.splitToolbarButton.hiddenSmallView
									button#previous.toolbarButton(title='Previous Page' tabindex='13' data-l10n-id='previous')
										span(data-l10n-id='previous_label') Previous
									.splitToolbarButtonSeparator
									button#next.toolbarButton(title='Next Page' tabindex='14' data-l10n-id='next')
										span(data-l10n-id='next_label') Next
								input#pageNumber.toolbarField(type='number' title='Page' value='1' min='1' tabindex='15' data-l10n-id='page' autocomplete='off')
								span#numPages.toolbarLabel
							#toolbarViewerRight
								button#openFile.toolbarButton.hidden.hiddenLargeView(title='Open File' tabindex='31' data-l10n-id='open_file')
									span(data-l10n-id='open_file_label') Open
								button#print.toolbarButton.hidden.hiddenMediumView(title='Print' tabindex='32' data-l10n-id='print')
									span(data-l10n-id='print_label') Print
								button#download.toolbarButton.hidden.hiddenMediumView(title='Save' tabindex='33' data-l10n-id='save')
									span(data-l10n-id='save_label') Save
								.verticalToolbarSeparator.hidden.hiddenMediumView
								#editorModeButtons.hidden.splitToolbarButton.toggled(role='radiogroup')
									button#editorFreeText.toolbarButton(disabled='disabled' title='Text' role='radio' aria-checked='false' tabindex='34' data-l10n-id='editor_free_text2')
										span(data-l10n-id='editor_free_text2_label') Text
									button#editorInk.toolbarButton(disabled='disabled' title='Draw' role='radio' aria-checked='false' tabindex='35' data-l10n-id='editor_ink2')
										span(data-l10n-id='editor_ink2_label') Draw
								#editorModeSeparator.hidden.verticalToolbarSeparator
								button#secondaryToolbarToggle.hidden.toolbarButton(title='Tools' tabindex='48' data-l10n-id='tools' aria-expanded='false' aria-controls='secondaryToolbar')
									span(data-l10n-id='tools_label') Tools
							#toolbarViewerMiddle
								.splitToolbarButton
									button#zoomOut.toolbarButton(title='Zoom Out' tabindex='21' data-l10n-id='zoom_out')
										span(data-l10n-id='zoom_out_label') Zoom Out
									.splitToolbarButtonSeparator
									button#zoomIn.toolbarButton(title='Zoom In' tabindex='22' data-l10n-id='zoom_in')
										span(data-l10n-id='zoom_in_label') Zoom In
								span#scaleSelectContainer.dropdownToolbarButton
									select#scaleSelect(title='Zoom' tabindex='23' data-l10n-id='zoom')
										option#pageAutoOption(title='' value='auto' selected='selected' data-l10n-id='page_scale_auto') Automatic Zoom
										option#pageActualOption(title='' value='page-actual' data-l10n-id='page_scale_actual') Actual Size
										option#pageFitOption(title='' value='page-fit' data-l10n-id='page_scale_fit') Page Fit
										option#pageWidthOption(title='' value='page-width' data-l10n-id='page_scale_width') Page Width
										option#customScaleOption(title='' value='custom' disabled='disabled' hidden='true')
										option(title='' value='0.5' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 50 }') 50%
										option(title='' value='0.75' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 75 }') 75%
										option(title='' value='1' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 100 }') 100%
										option(title='' value='1.25' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 125 }') 125%
										option(title='' value='1.5' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 150 }') 150%
										option(title='' value='2' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 200 }') 200%
										option(title='' value='3' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 300 }') 300%
										option(title='' value='4' data-l10n-id='page_scale_percent' data-l10n-args='{ "scale": 400 }') 400%
						// loadingBar
						#loadingBar
							.progress
								.glimmer
						// End loadingBar
					// End toolbarContainer
				#viewerContainer(tabindex='0')
					#viewer.pdfViewer
			// End mainContainer
			// dialogContainer
			#dialogContainer
				// passwordDialog
				dialog#passwordDialog
					.row
						label#passwordText(for='password' data-l10n-id='password_label') Enter the password to open this PDF file:
					.row
						input#password.toolbarField(type='password')
					.buttonRow
						button#passwordCancel.dialogButton
							span(data-l10n-id='password_cancel') Cancel
						button#passwordSubmit.dialogButton
							span(data-l10n-id='password_ok') OK
				// End passwordDialog
				// documentPropertiesDialog
				dialog#documentPropertiesDialog
					.row
						span#fileNameLabel(data-l10n-id='document_properties_file_name') File name:
						p#fileNameField(aria-labelledby='fileNameLabel') -
					.row
						span#fileSizeLabel(data-l10n-id='document_properties_file_size') File size:
						p#fileSizeField(aria-labelledby='fileSizeLabel') -
					.separator
					.row
						span#titleLabel(data-l10n-id='document_properties_title') Title:
						p#titleField(aria-labelledby='titleLabel') -
					.row
						span#authorLabel(data-l10n-id='document_properties_author') Author:
						p#authorField(aria-labelledby='authorLabel') -
					.row
						span#subjectLabel(data-l10n-id='document_properties_subject') Subject:
						p#subjectField(aria-labelledby='subjectLabel') -
					.row
						span#keywordsLabel(data-l10n-id='document_properties_keywords') Keywords:
						p#keywordsField(aria-labelledby='keywordsLabel') -
					.row
						span#creationDateLabel(data-l10n-id='document_properties_creation_date') Creation Date:
						p#creationDateField(aria-labelledby='creationDateLabel') -
					.row
						span#modificationDateLabel(data-l10n-id='document_properties_modification_date') Modification Date:
						p#modificationDateField(aria-labelledby='modificationDateLabel') -
					.row
						span#creatorLabel(data-l10n-id='document_properties_creator') Creator:
						p#creatorField(aria-labelledby='creatorLabel') -
					.separator
					.row
						span#producerLabel(data-l10n-id='document_properties_producer') PDF Producer:
						p#producerField(aria-labelledby='producerLabel') -
					.row
						span#versionLabel(data-l10n-id='document_properties_version') PDF Version:
						p#versionField(aria-labelledby='versionLabel') -
					.row
						span#pageCountLabel(data-l10n-id='document_properties_page_count') Page Count:
						p#pageCountField(aria-labelledby='pageCountLabel') -
					.row
						span#pageSizeLabel(data-l10n-id='document_properties_page_size') Page Size:
						p#pageSizeField(aria-labelledby='pageSizeLabel') -
					.separator
					.row
						span#linearizedLabel(data-l10n-id='document_properties_linearized') Fast Web View:
						p#linearizedField(aria-labelledby='linearizedLabel') -
					.buttonRow
						button#documentPropertiesClose.dialogButton
							span(data-l10n-id='document_properties_close') Close
				// End documentPropertiesDialog
				// printServiceDialog
				dialog#printServiceDialog(style='min-width: 200px;')
					.row
						span(data-l10n-id='print_progress_message') Preparing document for printing&mldr;
					.row
						progress(value='0' max='100')
						span.relative-progress(data-l10n-id='print_progress_percent' data-l10n-args='{ "progress": 0 }') 0%
					.buttonRow
						button#printCancel.dialogButton
							span(data-l10n-id='print_progress_close') Cancel
				// End printServiceDialog
			// End dialogContainer
		// End outerContainer
		// printContainer
		#printContainer
		// End printContainer
		input#fileInput.hidden(type='file')
		script(src='pdf.js/build/pdf.js')
		script(src='pdf_viewer.js?' + hash)
